{
  "mcpServers": {
    "lsmcp": {
      "command": "node",
      "args": [
        "dist/lsmcp.js"
      ]
    },
    "typescript": {
      "command": "node",
      "args": [
        "dist/typescript-mcp.js"
      ],
      "env": {
        "TSGO": "true"
      }
    },
    "readability": {
      "command": "npx",
      "args": [
        "-y",
        "@mizchi/readability",
        "--mcp"
      ]
    }
  }
}