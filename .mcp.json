{
  "mcpServers": {
    "typescript": {
      "command": "npx",
      "env": {
        "TSGO": "true"
      },
      "args": [
        "tsx",
        "src/mcp/typescript-mcp.ts"
      ]
    },
    "readability": {
      "command": "npx",
      "args": [
        "-y",
        "@mizchi/readability",
        "--mcp"
      ]
    },
    "lsp": {
      "command": "node",
      "args": [
        "dist/lsp-mcp.js"
      ],
      "env": {
        "LSP_COMMAND": "deno lsp"
      }
    }
  }
}